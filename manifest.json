{
  "manifest_version": 2,
  "name": "Coding Contest Reminder",
  "version": "1.0",
  "description": "A Firefox extension to remind you about coding contests",
  "icons": {
    "48": "icon.png",
    "130": "icon.png"
  },
  "permissions": [
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "130": "icon.png"
    },
    "default_popup": "index.html"
  }
}
